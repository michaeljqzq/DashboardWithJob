<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://msdn-dashboard.azurewebsites.net</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>False</LaunchSiteAfterPublish>
    <MSDeployServiceURL>msdn-dashboard.scm.azurewebsites.net:443</MSDeployServiceURL>
    <DeployIisAppPath>msdn-dashboard</DeployIisAppPath>
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <UserName>$msdn-dashboard</UserName>
    <_SavePWD>True</_SavePWD>
    <_DestinationType>AzureWebSite</_DestinationType>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="DataContext" Order="1" Enabled="False">
          <Destination Path="Data Source=tcp:msdn-dashboard.database.windows.net,1433;Initial Catalog=msdn-dashboard;User ID=msdn@msdn-dashboard.database.windows.net;Password=#Bugsfor$" Name="Data Source=tcp:msdn-dashboard.database.windows.net,1433;Initial Catalog=msdn-dashboard;User Id=msdn@msdn-dashboard.database.windows.net;Password=#Bugsfor$" />
          <Object Type="DbCodeFirst">
            <Source Path="DBMigration" DbContext="MSDNDashboard.DAL.DataContext, MSDNDashboard" MigrationConfiguration="MSDNDashboard.Migrations.Configuration, MSDNDashboard" Origin="Convention" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)DataContext-Web.config Connection String">
      <ParameterValue>Data Source=tcp:msdn-dashboard.database.windows.net,1433;Initial Catalog=msdn-dashboard;User Id=msdn@msdn-dashboard.database.windows.net;Password=#Bugsfor$</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
  <ItemGroup>
    <_ConnectionStringsToInsert Include="DataContext" />
  </ItemGroup>
</Project>